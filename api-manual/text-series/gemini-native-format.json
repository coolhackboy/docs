{
  "openapi": "3.0.0",
  "info": {
    "title": "Gemini Native Format",
    "description": "Native Gemini generateContent format for Google Gemini models.",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://api.apimart.ai",
      "description": "APIMart server"
    }
  ],
  "security": [
    {
      "BearerAuth": []
    }
  ],
  "paths": {
    "/v1beta/models/{model}:{method}": {
      "post": {
        "summary": "Gemini Native Format",
        "description": "Use the native Gemini request/response format.",
        "operationId": "generateGeminiContent",
        "tags": [
          "Text Series"
        ],
        "parameters": [
          {
            "name": "model",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "example": "gemini-2.5-pro"
            },
            "description": "Gemini model name."
          },
          {
            "name": "method",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "generateContent",
                "streamGenerateContent"
              ],
              "example": "generateContent"
            },
            "description": "Request method: `generateContent` or `streamGenerateContent`."
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GeminiGenerateContentRequest"
              },
              "examples": {
                "basic": {
                  "summary": "Basic request",
                  "value": {
                    "contents": [
                      {
                        "role": "user",
                        "parts": [
                          {
                            "text": "Hello, please introduce yourself"
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Content generated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GeminiGenerateContentResponse"
                },
                "examples": {
                  "success": {
                    "summary": "Successful response",
                    "value": {
                      "code": 200,
                      "data": {
                        "candidates": [
                          {
                            "content": {
                              "role": "model",
                              "parts": [
                                {
                                  "text": "Hello! I'm pleased to introduce myself."
                                }
                              ]
                            },
                            "finishReason": "STOP",
                            "index": 0
                          }
                        ],
                        "promptFeedback": {
                          "safetyRatings": [
                            {
                              "category": "HARM_CATEGORY_HATE_SPEECH",
                              "probability": "NEGLIGIBLE"
                            },
                            {
                              "category": "HARM_CATEGORY_DANGEROUS_CONTENT",
                              "probability": "NEGLIGIBLE"
                            },
                            {
                              "category": "HARM_CATEGORY_HARASSMENT",
                              "probability": "NEGLIGIBLE"
                            },
                            {
                              "category": "HARM_CATEGORY_SEXUALLY_EXPLICIT",
                              "probability": "NEGLIGIBLE"
                            }
                          ]
                        }
                      },
                      "usageMetadata": {
                        "promptTokenCount": 15,
                        "candidatesTokenCount": 29,
                        "totalTokenCount": 44,
                        "thoughtsTokenCount": 0,
                        "promptTokensDetails": [
                          {
                            "modality": "TEXT",
                            "tokenCount": 15
                          }
                        ]
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "API Key",
        "description": "All API endpoints require Bearer Token authentication.\n\nGet your API Key:\n\nVisit the API Key Management Page to get your API Key\n\nAdd it to the request header:\n\n```\nAuthorization: Bearer YOUR_API_KEY\n```"
      }
    },
    "schemas": {
      "GeminiGenerateContentRequest": {
        "type": "object",
        "required": [
          "contents"
        ],
        "properties": {
          "contents": {
            "type": "array",
            "description": "Conversation contents with roles and parts.",
            "items": {
              "$ref": "#/components/schemas/GeminiContent"
            }
          },
          "generationConfig": {
            "$ref": "#/components/schemas/GeminiGenerationConfig"
          },
          "safetySettings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GeminiSafetySetting"
            }
          }
        }
      },
      "GeminiContent": {
        "type": "object",
        "required": [
          "role",
          "parts"
        ],
        "properties": {
          "role": {
            "type": "string",
            "enum": [
              "user",
              "model"
            ],
            "example": "user"
          },
          "parts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GeminiPart"
            }
          }
        }
      },
      "GeminiPart": {
        "type": "object",
        "description": "A part can contain text or inline data.",
        "oneOf": [
          {
            "type": "object",
            "required": [
              "text"
            ],
            "properties": {
              "text": {
                "type": "string",
                "example": "Hello!"
              }
            }
          },
          {
            "type": "object",
            "required": [
              "inlineData"
            ],
            "properties": {
              "inlineData": {
                "$ref": "#/components/schemas/GeminiInlineData"
              }
            }
          }
        ]
      },
      "GeminiInlineData": {
        "type": "object",
        "required": [
          "mimeType",
          "data"
        ],
        "properties": {
          "mimeType": {
            "type": "string",
            "example": "image/png"
          },
          "data": {
            "type": "string",
            "description": "Base64-encoded data.",
            "example": "BASE64_ENCODED_IMAGE"
          }
        }
      },
      "GeminiGenerationConfig": {
        "type": "object",
        "properties": {
          "temperature": {
            "type": "number",
            "minimum": 0,
            "maximum": 2,
            "example": 0.7
          },
          "topP": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "example": 0.9
          },
          "topK": {
            "type": "integer",
            "minimum": 1,
            "example": 40
          },
          "maxOutputTokens": {
            "type": "integer",
            "minimum": 1,
            "example": 512
          },
          "stopSequences": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "GeminiSafetySetting": {
        "type": "object",
        "properties": {
          "category": {
            "type": "string",
            "example": "HARM_CATEGORY_DANGEROUS_CONTENT"
          },
          "threshold": {
            "type": "string",
            "example": "BLOCK_MEDIUM_AND_ABOVE"
          }
        }
      },
      "GeminiGenerateContentResponse": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "example": 200
          },
          "data": {
            "$ref": "#/components/schemas/GeminiGenerateContentResult"
          },
          "usageMetadata": {
            "$ref": "#/components/schemas/GeminiUsageMetadata"
          }
        }
      },
      "GeminiGenerateContentResult": {
        "type": "object",
        "properties": {
          "candidates": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GeminiCandidate"
            }
          },
          "promptFeedback": {
            "$ref": "#/components/schemas/GeminiPromptFeedback"
          }
        }
      },
      "GeminiCandidate": {
        "type": "object",
        "properties": {
          "content": {
            "$ref": "#/components/schemas/GeminiContent"
          },
          "finishReason": {
            "type": "string",
            "example": "STOP"
          },
          "index": {
            "type": "integer",
            "example": 0
          },
          "safetyRatings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GeminiSafetyRating"
            }
          }
        }
      },
      "GeminiPromptFeedback": {
        "type": "object",
        "properties": {
          "safetyRatings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GeminiSafetyRating"
            }
          }
        }
      },
      "GeminiSafetyRating": {
        "type": "object",
        "properties": {
          "category": {
            "type": "string",
            "example": "HARM_CATEGORY_DANGEROUS_CONTENT"
          },
          "probability": {
            "type": "string",
            "example": "NEGLIGIBLE"
          }
        }
      },
      "GeminiUsageMetadata": {
        "type": "object",
        "properties": {
          "promptTokenCount": {
            "type": "integer",
            "example": 15
          },
          "candidatesTokenCount": {
            "type": "integer",
            "example": 29
          },
          "totalTokenCount": {
            "type": "integer",
            "example": 44
          },
          "thoughtsTokenCount": {
            "type": "integer",
            "example": 0
          },
          "promptTokensDetails": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GeminiTokenDetail"
            }
          }
        }
      },
      "GeminiTokenDetail": {
        "type": "object",
        "properties": {
          "modality": {
            "type": "string",
            "example": "TEXT"
          },
          "tokenCount": {
            "type": "integer",
            "example": 15
          }
        }
      }
    }
  }
}
